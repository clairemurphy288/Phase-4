<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Phase 4</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>Store Sales Overview</h1>
    <table>
        <thead>
            <tr>
                <th>Store ID</th>
                <th>Store Name</th>
                <th>Manager</th>
                <th>Revenue</th>
                <th>Incoming Revenue</th>
                <th>Incoming Orders</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(item => { %>
                <tr>
                    <td><%= item.store_id %></td>
                    <td><%= item.sname %></td>
                    <td><%= item.manager %></td>
                    <td><%= item.revenue %></td>
                    <td><%= item.incoming_revenue %></td>
                    <td><%= item.incoming_orders %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>